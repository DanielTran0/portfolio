{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"plugin:react/recommended",
		"airbnb",
		"plugin:prettier/recommended"
	],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"no-console": "off",
		"import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
		"no-useless-escape": "off",
		"react/jsx-props-no-spreading": "off"
	}
}
